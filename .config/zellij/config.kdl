keybinds clear-defaults=true {
    normal {
        bind "Ctrl o" { GoToPreviousTab; }
        bind "Ctrl a" { GoToNextTab; }
        bind "Ctrl t" { NewTab; SwitchToMode "Normal"; }
        bind "Ctrl w" { CloseTab; SwitchToMode "Normal"; }
    }
}

on_force_close "quit"
pane_frames false
default_layout "compact"
auto_layout false
